private void isAnagramUsingSort(String s1, String s2){
		char[] c1 = new char[26];
		c1 = s1.trim().toLowerCase().toCharArray();
		char[] c2 = new char[26];
		c2 = s2.trim().toLowerCase().toCharArray();
		Arrays.sort(c1);
		Arrays.sort(c2);
		if(Arrays.equals(c1, c2)){
			System.out.println("Anagrams");
		}
		else{
			System.out.println("Not Anagram");
		}
	}
