class Solution { 
    public int rob(int[] nums) {
        int n = nums.length;
        if(n == 1) {
            return nums[0];
        }
        int first  = nums[0], second = Math.max(first, nums[1]);
        if(n == 2) {
            return Math.max(first, second);
        }
        for(int i = 2; i < n; i++) {
            int temp = Math.max(nums[i] + first, second);
            first = second;
            second = temp;
        }

        return second;
    }
}
